{
    "name": "@xilution/xilution-graphql-example",
    "version": "1.0.0",
    "description": "An example showing how to build a graphql server using Xilution's Beagily and Fox",
    "scripts": {
        "build": "tsc -p tsconfig.json && webpack-cli --mode production --config webpack.config.js",
        "clean": "rm -rf built dist",
        "docker:build": "./scripts/docker-build.sh",
        "docker:publish": "./scripts/docker-publish.sh",
        "docker:start": "./scripts/docker-start.sh",
        "docker:stop": "./scripts/docker-stop.sh",
        "xln:activate-fox": "./scripts/xln-activate-fox.sh",
        "xln:create-fox-instance": "./scripts/xln-create-fox-instance.sh",
        "xln:deactivate-fox": "./scripts/xln-deactivate-fox.sh",
        "xln:delete-fox-instance": "./scripts/xln-delete-fox-instance.sh",
        "xln:deprovision-fox-instance": "./scripts/xln-deprovision-fox-instance.sh",
        "xln:provision-fox-instance": "./scripts/xln-provision-fox-instance.sh",
        "xln:restart-fox-instance": "./scripts/xln-restart-fox-instance.sh",
        "xln:show-fox-activation": "./scripts/xln-show-fox-activation.sh",
        "xln:show-fox-instance-status": "./scripts/xln-show-fox-instance-status.sh",
        "xln:show-fox-instances": "./scripts/xln-show-fox-instances.sh",
        "lint": "tslint \"src/**/*.ts\"",
        "test": "echo \"not yet\"",
        "start": "nodemon --watch src --exec ts-node src/index.ts",
        "debug": "nodemon --watch src --exec ts-node --inspect --debug-brk src/index.ts",
        "verify": "yarn lint && yarn test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xilution/xilution-graphql-example"
    },
    "bugs": {
        "url": "https://github.com/xilution/xilution-graphql-example/issues"
    },
    "homepage": "https://github.com/xilution/xilution-graphql-example",
    "dependencies": {
        "@hapi/boom": "^7.4.2",
        "@hapi/catbox-memory": "^4.1.0",
        "@hapi/hapi": "^18.3.1",
        "@types/graphql": "^14.2.3",
        "@types/hapi__boom": "^7.4.0",
        "@types/hapi__catbox-memory": "^4.1.1",
        "@types/hapi__hapi": "^18.2.5",
        "@types/lodash": "^4.14.134",
        "@types/object-hash": "^1.3.0",
        "apollo-server-hapi": "^2.7.2",
        "aws-sdk": "^2.447.0",
        "axios": "^0.19.0",
        "graphql": "^14.4.2",
        "lodash": "^4.17.11",
        "object-hash": "^1.3.1",
        "query-string": "^6.8.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@types/node": "^11.13.5",
        "husky": "^2.1.0",
        "nodemon": "^1.18.11",
        "ts-loader": "^6.0.4",
        "ts-node": "^8.1.0",
        "tslint": "^5.16.0",
        "typescript": "^3.4.4",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.0"
    },
    "engines": {
        "node": ">=10.16.3",
        "yarn": ">=1.3.2",
        "npm": ">=5.6.0"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "author": "Todd Brunia <tbrunia@gmail.com> (http://www.github.com/tbrunia)",
    "license": "MIT"
}
